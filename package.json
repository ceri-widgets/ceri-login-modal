{
  "name": "ceri-login-modal",
  "description": "A simple, themed login-modal",
  "version": "0.0.4",
  "homepage": "https://github.com/ceri-widgets",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "license": "MIT",
  "main": "login-modal.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ceri-widgets/ceri-login-modal"
  },
  "files": [
    "*.js",
    "*.scss"
  ],
  "engines": {
    "node": "*"
  },
  "dependencies": {
    "ceri": "^1.0.14",
    "ceri-icon": "^0.0.3",
    "ceri-modal": "^0.0.3"
  },
  "devDependencies": {
    "ceri-materialize": "^1.0.0",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.12.3",
    "gh-pages": "^0.12.0",
    "script-runner": "^0.1.6",
    "ceri-dev-server": "^1.0.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.3",
    "ceri-compiler": "^1.0.0"
  },
  "keywords": [
    "login-modal",
    "component",
    "ceri"
  ],
  "readmeFilename": "README.md",
  "scripts": {
    "build:coffee": "coffee -c -o . src/*.coffee",
    "build:bundle": "ceri-compiler materialize.js login-modal-component.js",
    "build": "run-npm build:*",
    "dev": "ceri-dev-server",
    "watch": "ceri-dev-server --test --watch",
    "test": "ceri-dev-server --test",
    "preversion": "npm test",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags && npm publish",
    "ghpages": "ceri-dev-server --static static/ && gh-pages -d static"
  }
}
